{
  "name": "crypto-residency",
  "version": "1.0.0",
  "private": true,
  "description": "Autonomous Cryptographic Residency Engine",
  "scripts": {
    "day:run": "ts-node -r tsconfig-paths/register --project tsconfig.json apps/orchestrator/index.ts",
    "grade": "ts-node -r tsconfig-paths/register --project tsconfig.json apps/tools/grade-submission.ts",
    "homework-specs:init": "ts-node -r tsconfig-paths/register --project tsconfig.json apps/tools/generate-homework-specs.ts --pretty",
    "homework-specs:regen": "ts-node -r tsconfig-paths/register --project tsconfig.json apps/tools/generate-homework-specs.ts --pretty --force",
    "pre-homework-specs:init": "ts-node -r tsconfig-paths/register --project tsconfig.json apps/tools/generate-homework-specs.ts --pre --pretty",
    "pre-homework-specs:regen": "ts-node -r tsconfig-paths/register --project tsconfig.json apps/tools/generate-homework-specs.ts --pre --pretty --force",
    "structured-inputs:init": "npm run homework-specs:init",
    "structured-inputs:regen": "npm run homework-specs:regen",
    "build": "tsc --project tsconfig.json",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "dotenv": "^16.4.5",
    "openai": "^4.52.7"
  },
  "devDependencies": {
    "@types/node": "^20.14.10",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.3"
  }
}
